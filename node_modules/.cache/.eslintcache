Data(data);\n    }\n\n    // const thumbUrl = inputData.thumbUrl;\n\n    return(\n        <>\n            <section>\n                <div>\n                    {\n                        inputData.map((item, index) => {\n                            return(\n                                <div key={index} style={style.component}>\n                                    <div className=\"thumbNail\">\n                                        <a href={item.url}>\n                                            <img\n                                                src={item.thumbUrl}\n                                                alt=\"thumbNail\"\n                                                style={style.thumb}\n                                            />\n                                        </a>\n                                    </div>\n                                    <div>\n                                        <a href={item.url} style={style.title}>{item.title}</a>\n                                        <a href={item.url} style={style.name}>{item.name}</a>\n                                    </div>\n                                </div>\n                            )\n                        })\n                    }\n                </div>\n            </section>\n        </>\n    )\n}\n\nconst style = {\n    component : {\n        display: 'grid',\n\n        gridColumnGap: '23px',\n        gridRowGap: '50px',\n        textAlign:'center',\n        color:'white'\n    },\n    thumb:{\n        width:'330px',\n        height:'440px'\n    },\n    title:{\n        fontsize: '24px',\n        textDecoration:'none'\n\n    },\n    name:{\n        fontsize: '18px',\n        textDecoration:'none'\n    }\n}","/Users/jeongnayeong/Desktop/2021_KHU_dicon_/outsourcing_2021_KHU_dicon/src/components/firebase/getThumbNail.js",[],"/Users/jeongnayeong/Desktop/2021_KHU_dicon_/outsourcing_2021_KHU_dicon/src/components/works/showThumbNail.js",[],"/Users/jeongnayeong/Deskto